{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/ui/*": [
        "./components/ui/*"
      ],
      "@/page/*": [
        "./components/page/*"
      ],
      "@/component/*": [
        "./components/*"
      ],
      "@/app/*": [
        "./app/*"
      ],
      "@/assets/*": [
        "./assets/*"
      ],
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}